{
  "expo": {
    "name": "Scaneat",
    "slug": "scaneat",
    "version": "1.0.0",
    "description": "Food Barcode Scanner & Health Analyzer",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
