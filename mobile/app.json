      },
      "permissions": [
        "CAMERA",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "allowBackup": true,
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "16826cab-56d6-4c55-9440-f6e201747ecd"
      }
    },
    "owner": "brilworksdigitalmvp",
    "runtimeVersion": {
      "policy": "appVersion"
